<h1>作品情報編集</h1>
<%= form_with model: @item, local: true do |f| %>
  <%= f.label :"作品画像" %>
  <%= f.attachment_field :item_images_images, multiple: true %>
  
  <%= f.label :"タイトル" %>
  <%= f.text_field :title %>
  
  <%= f.label :"説明文" %>
  <%= f.text_area :body %>
  
  <%= f.label :"タグ" %>
  <%= f.text_field :name %>
  
  <%= f.submit "変更を保存"%>
<% end %>