<table class="table">
  <% items.each do |item| %>
    <tr>
      <td>
        <% item.item_images.each do |item_image| %>
          <%= link_to(item) do %>
            <%= attachment_image_tag item_image, :image, size: "130x100", format: "jpg" %>
          <% end %>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>